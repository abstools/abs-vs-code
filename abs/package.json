{
    "name": "abs",
    "displayName": "abs",
    "description": "Support for the Abs language",
    "icon": "images/ABS_logo.png",
    "version": "0.0.2",
    "publisher": "abs",
    "repository": "https://github.com/abstools/abs-vs-code/",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "abs",
            "aliases": ["Abs", "abs"],
            "extensions": [".abs"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "abs",
            "scopeName": "source.abs",
            "path": "./syntaxes/abs.tmLanguage.json"
        }]
    }
}
